{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",  // TS文件编译后会放入到此文件夹内
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*",  //根据项目的路径指定
        "src/types/*"      //根据项目的路径指定
      ]
    }
  },
  "include": [
    "src/**/*"                //根据项目的路径指定
  ]
}
